doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href="../style.css" rel='stylesheet' type='text/css')
    title Search Results
  body
    table.results
      each question in relevantQuestions
        tr: strong: td(style="color: orange; font-size: 1.5rem; text-indent: 50px; padding: 0") #{question.questionSubject}
        td(style="") #{question.questionText}
