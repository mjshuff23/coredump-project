extends layout.pug
block content
  div
    p(text-align="center") Registered Users:
  div.usersWrapper
    each user in users
      div.userBox
        div.userName2
          if signedIn
            a(href=`/users/${user.id}`) #{user.userName}
          else
            span #{user.userName}
        div.cred User: #{user.id}
        img.avatar2(src=`${user.avatar}` height="50")
  br
  div
    p(text-align="center") Developers on this project:
  div.usersWrapper
    div.userBox
      div.userName2
        a(href=`https://github.com/ahan8927`) Aaron Hanson
      div.cred
      img.avatar2(src=`https://avatars2.githubusercontent.com/u/67394227?s=460&u=d065c60f5b20b9f02ba844dd323875a745a39309&v=4` height="50")
    div.userBox
      div.userName2
        a(href=`https://github.com/johnegus`) John Hiestand
